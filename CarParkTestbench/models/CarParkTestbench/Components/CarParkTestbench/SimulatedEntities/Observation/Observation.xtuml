INSERT INTO O_TFR
	VALUES ("7555b918-dbfd-4cb7-8e5f-80d8fbd65a72",
	"765a75f4-57e1-4717-9b3e-fc9c61b630fb",
	'Delete',
	'Dispose of the client instances when the test sequence service no longer needs them.',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'select one req related by self->QueryTicketRequest[R4];
if ( not_empty req )
  unrelate req from self across R4;
  delete object instance req;
end if;
select one ti related by self->TicketIssued[R4];
if ( not_empty ti )
  unrelate ti from self across R4;
  delete object instance ti;
end if;
select one bs related by self->BarrierStatus[R4];
if ( not_empty bs )
  unrelate bs from self across R4;
  delete object instance bs;
end if;
select one bv related by self->BalanceValue[R4];
if ( not_empty bv )
  unrelate bv from self across R4;
  delete object instance bv;
end if;

delete object instance self;',
	1,
	'',
	"981e7c87-325e-4efb-ba73-a51e6e697322",
	0);
