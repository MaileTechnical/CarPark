INSERT INTO O_TFR
	VALUES ("be73cadd-dde6-4c6a-9072-f4ed26103cde",
	"765a75f4-57e1-4717-9b3e-fc9c61b630fb",
	'Observe',
	'This operation will be invoked when the counterpart instance is encountered by the sequencing service.',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'select one tr related by self->QueryTicketRequest[R4];
if ( not_empty tr )
  tr.CheckRequest();
else
  select one bs related by self->BarrierStatus[R4];
  if ( not_empty bs )
  	bs.CheckStatus(); 
  else
  	select one ti related by self->TicketIssued[R4];
  	if ( not_empty ti )
  	  ti.CheckIssued();
    else
      select one bv related by self->BalanceValue[R4];
      if ( not_empty bv )
        bv.CheckBalance();
      end if;
  	end if;
  end if;
end if;',
	1,
	'',
	"d7ab8883-a80f-4024-88f2-1ab6a93ad335",
	0);
