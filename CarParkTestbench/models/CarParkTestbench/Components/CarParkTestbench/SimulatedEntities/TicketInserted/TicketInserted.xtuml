INSERT INTO O_TFR
	VALUES ("cda2beec-3e37-4d70-be67-8417cc30c9a9",
	"0886984c-f53c-40d8-a864-555ceb665749",
	'Inserted',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'LOG::LogInfo( message:"Ticket inserted at " + self.location + self.standType );

select any stay from instances of Stay;
if ( self.standType == "Pay" )
  send Payer::InsertedTicket( Location:self.location, TicketNumber:stay.ticketNumber);
else
  send ExitStand::InsertedTicket( Location:self.location, TicketNumber:stay.ticketNumber );
end if;

select one stimulus related by self->Stimulus[R3];
stimulus.Complete();
',
	1,
	'',
	"165f67eb-f21a-4f39-9700-ba93b069b8e7",
	0);
