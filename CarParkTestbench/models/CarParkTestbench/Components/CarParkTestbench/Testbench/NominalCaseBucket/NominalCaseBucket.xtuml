INSERT INTO O_TFR
	VALUES ("d46c90fe-f810-493d-bb6f-190f4fc6db0b",
	"cc521408-6dcf-4740-b70f-b30d4d5e1f14",
	'RunBucketTests',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'select one bucket related by self->TestBucket[R7];
send TESTSEQ::NameTestBucket( bucketId:bucket.bucketId, bucketLabel:"CarPark_Bucket_1" );
//send TESTSEQ::SetTestBucketResolution( bucketId:bucket.bucketId, resolution:timerResolution::second );

// Use Case UC1 
send TESTSEQ::CreateTestCase( bucketId:bucket.bucketId, caseLabel:"UC1" ); 
send TESTSEQ::CreateStimulusSequence( caseLabel:"UC1", seqLabel:"seq01" );
stId = VehicleArrived::Create( seqLabel:"seq01", location:"North", stand:"Entry" );
QueryTicketRequest::CreatePolling( stimulus:stId, location:"North", pollInterval:1, timeLimit:5 );
stId = TicketRequested::Create( seqLabel:"seq01", location:"North" );
TicketIssued::CreatePolling( stimulus:stId, location:"North", pollInterval:1, timeLimit:5 );
stId = TicketTaken::Create( seqLabel:"seq01", location:"North", stand:"Entry" );
BarrierStatus::CreatePolling( stimulus:stId, location:"North", stand:"Entry", isopen:True, pollInterval:1, timeLimit:5 );
stId = VehicleDeparture::Create( seqLabel:"seq01", location:"North", stand:"Entry" );
BarrierStatus::CreatePolling( stimulus:stId, location:"North", stand:"Entry", isopen:False, pollInterval:1, timeLimit:5 );


send TESTSEQ::CreateTestCase( bucketId:bucket.bucketId, caseLabel:"UC5" ); 
send TESTSEQ::CreateStimulusSequence( caseLabel:"UC5", seqLabel:"seq05" );
stId = FastForward::Create( seqLabel:"seq05", hours:5, minutes:0, seconds:0 );
stId = TicketInserted::Create( seqLabel:"seq05", location:"1st Floor", stand:"Pay", number:1 );
BalanceValue::CreatePolling( stimulus:stId, location:"1st Floor", pollInterval:1, timeLimit:30 );



bucket.Initialized();
',
