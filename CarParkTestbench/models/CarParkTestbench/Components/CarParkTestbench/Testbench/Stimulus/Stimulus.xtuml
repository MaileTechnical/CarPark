INSERT INTO O_TFR
	VALUES ("a3a29c93-9044-44e5-8f61-97f557dc69be",
	"0279c804-9fcf-4db2-bcfa-077aadf90fb2",
	'Inject',
	'This operation will be invoked when the stimulus is encountered by the sequencing service.',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'select one va related by self->VehicleArrived[R3];
if ( not_empty va )
  va.VehicleArrival();
else
  select one tr related by self->TicketRequested[R3];
  if ( not_empty tr )
    tr.Requested();
  else
  	select one tt related by self->TicketTaken[R3];
  	if ( not_empty tt )
  	  tt.Collected();
  	else
      select one vd related by self->VehicleDeparture[R3];
      if ( not_empty vd )
        vd.VehicleNotThere();
      else
        select one ff related by self->FastForward[R3];
        if ( not_empty ff )
          ff.Advance();
        else
          select one ti related by self->TicketInserted[R3];
          if ( not_empty ti )
            ti.Inserted();
          end if;
        end if;
      end if;
    end if;
  end if;
end if;
',
	1,
	'',
	"d0618c93-5a38-4e92-94b0-b4264bc79346",
	0);
